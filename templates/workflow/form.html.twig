{% extends 'workflow/base.html.twig' %}
{% block content %}
        <h1 class="cover-heading">{{ titel }}</h1>
<p>{{ text }}</p>
    <ul>
        {% if errors is defined %}
        {% for error in errors %}
            <li>{{ error.message }}</li>
        {% endfor %}
        {% endif %}
    </ul>
    {{ form_start(form, {'attr': {'novalidate': 'novalidate'}}) }}
    {{ form_widget(form) }}
    {{ form_end(form) }}
{% endblock %}